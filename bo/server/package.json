{
  "name": "server",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:production": "NODE_ENV=production node src/index.js",
    "dev": "nodemon src/index.js",
    "db:init": "node scripts/db-init.js",
    "db:setup": "node scripts/setup-database.js",
    "db:setup:production": "node scripts/setup-database-production.js",
    "db:reset": "node scripts/setup-database.js",
    "db:test": "node scripts/test-connection.js",
    "db:add-invoices": "node scripts/add-invoices-table.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:integration": "NODE_ENV=test jest --testMatch='**/tests/integration/**/*.test.js'",
    "test:unit": "NODE_ENV=test jest --testMatch='**/tests/**/*.test.js' --ignore='**/tests/integration/**'",
    "db:backup": "node scripts/backup-database.js",
    "vercel-build": "echo 'Build complete'"
  },
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@paypal/paypal-server-sdk": "^1.1.0",
    "axios-cookiejar-support": "^6.0.4",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mysql-session": "^3.0.3",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.14.2",
    "nodemailer": "^7.0.5",
    "sanitize-html": "^2.17.0",
    "stripe": "^18.3.0",
    "tough-cookie": "^5.1.2",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.12"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
